{
  "name": "next-custom-server-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env FORCE_COLOR=3 vite --open --host",
    "dev1": "vite-node -w ./src/server/dev.ts",
    "build": "cross-env IS_BUILDING=true next build",
    "build:esbuild": "cross-env NODE_ENV=production ts-node-dev -r tsconfig-paths/register --project ./tsconfig.server.json --transpile-only ./_esbuild.ts",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "lint": "next lint",
    "docker": "docker build --progress=plain --build-arg PORT=3000 -t nextjs-docker . && docker run --rm -p 9002:9002 -p 3000:3000 nextjs-docker",
    "generate:devops": "ts-node-dev --project ./tsconfig.server.json --transpile-only ./src/server/config/devops.generate.ts",
    "postgenerate:devops": "prettier --write ./src/server/config/devops.schema.json",
    "generate:manager": "ts-node-dev --project ./tsconfig.server.json --transpile-only ./src/server/config/manager.generate.ts",
    "postgenerate:manager": "prettier --write ./src/server/config/manager.schema.json",
    "prepare": "is-ci || yarn prepare:1",
    "prepare:1": "husky install && chmod +x .husky/*",
    "ngrok": "./ngrok.exe http 9002",
    "proto": "ts-node-dev -r tsconfig-paths/register --project ./tsconfig.proto.json --transpile-only ./src/generateProto.ts"
  },
  "lint-staged": {
    "./**/*.{css,scss,md,json}": "prettier --write",
    "./**/*.{js,jsx,tsx,ts}": "eslint --fix"
  },
  "dependencies": {
    "@fastify/express": "^2.1.0",
    "@fastify/websocket": "^7.1.1",
    "@tanstack/react-query": "^4.18.0",
    "@trpc/client": "^10.8.2",
    "@trpc/next": "^10.8.2",
    "@trpc/react-query": "^10.8.2",
    "@trpc/server": "^10.8.2",
    "chalk": "^4",
    "cookies-next": "^2.1.1",
    "date-fns": "^2.29.3",
    "deepmerge": "^4.2.2",
    "express": "^4.18.2",
    "fastify": "^4.10.2",
    "fastify-plugin": "^4.3.0",
    "i18next": "^22.0.6",
    "jose": "^4.11.2",
    "js-yaml": "^4.1.0",
    "long": "^5.2.1",
    "next": "13.0.5",
    "next-i18next": "^13.0.0",
    "next-themes": "^0.2.1",
    "nice-grpc": "^2.1.0",
    "protobufjs": "^7.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.0.0",
    "reflect-metadata": "^0.1.13",
    "replace-in-file": "^6.3.5",
    "rxjs": "^7.5.7",
    "sharp": "^0.31.2",
    "swagger-ui-express": "^4.6.0",
    "timsy": "^2.5.0",
    "transitory": "^2.2.0",
    "trpc-openapi": "^1.1.1",
    "typeorm": "^0.3.10",
    "undici": "^5.13.0",
    "uuid": "^9.0.0",
    "ws": "^8.11.0",
    "zod": "^3.20.2",
    "zod-to-json-schema": "^3.20.2"
  },
  "devDependencies": {
    "@anatine/esbuild-decorators": "^0.2.19",
    "@swc/core": "^1.3.20",
    "@types/cookie": "^0.5.1",
    "@types/deepmerge": "^2.2.0",
    "@types/express": "^4.17.14",
    "@types/glob": "^8.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@typescript-eslint/typescript-estree": "^5.44.0",
    "@vanilla-extract/babel-plugin": "^1.2.0",
    "@vanilla-extract/css": "^1.9.2",
    "@vanilla-extract/next-plugin": "^2.1.1",
    "@vanilla-extract/recipes": "^0.3.0",
    "cross-env": "^7.0.3",
    "download-cli": "^1.1.1",
    "esbuild": "^0.15.15",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "esm": "^3.2.25",
    "glob": "^8.1.0",
    "grpc-tools": "^1.12.3",
    "husky": "^8.0.2",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.0.4",
    "prettier": "^2.8.0",
    "rimraf": "^4.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "ts-proto": "^1.138.0",
    "typescript": "4.9.3",
    "vite": "^3.2.4",
    "vite-node": "^0.25.3",
    "vite-plugin-node": "^1.0.0"
  }
}
