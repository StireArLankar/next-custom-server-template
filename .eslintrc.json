{
  "plugins": ["prettier"],
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "prettier/prettier": ["warn", {}, { "usePrettierrc": true }],
    "import/order": [
      "warn",
      {
        "newlines-between": "always",
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index"
        ],
        "alphabetize": {
          "order": "asc",
          "caseInsensitive": true
        }
      }
    ]
  }
}
